#include <iostream>
#include <cmath>
using namespace std;

int
main (void)
{
int n, a, i, x,d;
cout << "N= ";
cin >> n;
cout << "A= ";
cin >> a;
cout << "D= ";
cin >> d;

int *mas = new int[n];
cout << a << " " << ends;
for (int i = 1; i < n; i++)
{
x = pow (d, i);
mas[i] = a * x;
cout << mas[i] << " ";
}
return 0;
}
